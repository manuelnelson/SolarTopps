        <div class="input-group"><input id="company" class="form-control"
                                        name="company" type="text" placeholder="Company"/>
        </div>
    </div>
    <div class="col-md-5">
        <div class="input-group"><input id="title" class="form-control"
                                        name="title" type="text" placeholder="Title"/>
        </div>
    </div>
</div>
<div class="form-group">
    <div class="col-md-5">
        <div class="input-group"><input id="department" class="form-control"
                                        name="department" type="text" placeholder="Department"/>
