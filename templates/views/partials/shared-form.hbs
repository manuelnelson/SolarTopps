<form class="form-horizontal"><fieldset><!-- Form Name --> <legend>(*Required)</legend>
    {{> generic-form}}
    <!-- Select Basic -->
    <div class="form-group"><label class="col-md-4 control-label" for="propertyType">* Property Type</label>
        <div class="col-md-5"><select id="propertyType" class="form-control" name="propertyType">
            <option value="">-</option>
            <option value="">Residential Single Story</option>
            <option value="">Residential Double Story</option>
            <option value="">Commercial Single Story</option>
            <option value="">Commercial Multiple Story</option>
            <option value="">Educational Institution</option>
            <option value="">Government Facility</option>
            <option value="">New Home Construction</option>
        </select></div>
    </div>
    <!-- Button Drop Down -->
    <div class="form-group"><label class="col-md-4 control-label" for="Former Solar Topps Customer">Are You An Existing Solar Topps Customer? If Not, Please Answer The Questions Below</label>
    </div>
    <!-- Text input-->
    <div class="form-group"><label class="col-md-4 control-label" for="whoInstalled">Who Installed Your System?</label>
        <div class="col-md-5"><input id="whoInstalled" class="form-control input-md" name="whoInstalled" type="text" placeholder="Company Name" /></div>
    </div>
    <!-- Text input-->
    <div class="form-group"><label class="col-md-4 control-label" for="whenInstalled">When Was Your System Installed?</label>
        <div class="col-md-5"><input id="whenInstalled" class="form-control input-md" name="whenInstalled" type="text" placeholder="Installation Date" /></div>
    </div>
    <!-- Button Drop Down -->
    <div class="form-group"><label class="col-md-4 control-label" for="purchased">* Is Your System Purchased or Leased?</label>
        <div class="col-md-4">
            <div class="input-group"><input id="purchased" class="form-control" name="purchased" required="" type="text" placeholder="" />
                <div class="input-group-btn"><button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown"> Choose One </button>
                    <ul class="dropdown-menu pull-right">
                        <li><a href="#">Purchase</a></li>
                        <li><a href="#">Lease</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- Text input-->
    <div class="form-group"><label class="col-md-4 control-label" for="monitoring">What Type of Monitoring Do You Have?</label>
        <div class="col-md-5"><input id="monitoring" class="form-control input-md" name="monitoring" type="text" placeholder="Company Name" /> <span class="help-block">If None, Please Write N/A</span></div>
    </div>
    <!-- Text input-->
    <div class="form-group"><label class="col-md-4 control-label" for="size">What is the Size of Your System?</label>
        <div class="col-md-5"><input id="size" class="form-control input-md" name="size" type="text" placeholder="# kW" /></div>
    </div>
    <!-- Button Drop Down -->
    <div class="form-group"><label class="col-md-4 control-label" for="systemMounting">System Mounting</label>
        <div class="col-md-5">
            <div class="input-group"><input id="systemMounting" class="form-control" name="systemMounting" type="text" placeholder="" />
                <div class="input-group-btn"><button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown"> Choose </button>
                    <ul class="dropdown-menu pull-right">
                        <li><a href="#">Roof</a></li>
                        <li><a href="#">Ground Mounted</a></li>
                        <li><a href="#">Parking Shade / Structure</a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- Text input-->
    <div class="form-group"><label class="col-md-4 control-label" for="numberOfPanels">How Many Panels Are On Your System?</label>
        <div class="col-md-5"><input id="numberOfPanels" class="form-control input-md" name="numberOfPanels" type="text" placeholder="Panel Qty" /></div>
    </div>
    <!-- Text input-->
    <div class="form-group"><label class="col-md-4 control-label" for="typeOfPanels">What Type of Panels Are On Your System?</label>
        <div class="col-md-5"><input id="typeOfPanels" class="form-control input-md" name="typeOfPanels" type="text" placeholder="Company Name" /> <span class="help-block">(SunPower, Hyundai, Hanwha etc)</span></div>
    </div>
    <!-- Text input-->
    <div class="form-group"><label class="col-md-4 control-label" for="numberOfInverters">How Many Inverters Does Your System Have?</label>
        <div class="col-md-5"><input id="numberOfInverters" class="form-control input-md" name="numberOfInverters" type="text" placeholder="Inverty Qty" /></div>
    </div>
    <!-- Text input-->
    <div class="form-group"><label class="col-md-4 control-label" for="typeOfInverters">What Type of Inverters Are On Your System?</label>
        <div class="col-md-5"><input id="typeOfInverters" class="form-control input-md" name="typeOfInverters" type="text" placeholder="Company Name" /></div>
    </div>
    <!-- Select Multiple -->
    <div class="form-group"><label class="col-md-4 control-label" for="hearAbout">How Did You Hear About Us?</label>
        <div class="col-md-5"><select id="hearAbout" class="form-control" multiple="multiple" name="hearAbout">
            <option value="">Google</option>
            <option value="">Bing</option>
            <option value="">Yahoo</option>
            <option value="">Facebook</option>
            <option value="">Twitter</option>
            <option value="">LinkedIn</option>
            <option value="">Youtube</option>
            <option value="">Direct Mailer</option>
            <option value="">Door Hanger</option>
            <option value="">Canvasser</option>
            <option value="">Email</option>
            <option value="">Website Banner</option>
            <option value="">Radio</option>
            <option value="">Television</option>
            <option value="">Trade Show</option>
            <option value="">Magazine</option>
            <option value="">Newspaper</option>
            <option value="">Other</option>
        </select></div>
    </div>
    <!-- Textarea -->
    <div class="form-group"><label class="col-md-4 control-label" for="comments">Comments</label>
        <div class="col-md-4"><textarea id="comments" class="form-control" name="comments">Provide any additional information you feel necessary</textarea></div>
    </div>
</fieldset></form>
